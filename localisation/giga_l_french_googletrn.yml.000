    #########################################
    qso_intensity_none:0 "QS-O : pas de charge"
    qso_intensity_none_desc:0 "Le QS-O ne se charge pas. Toute sa production est convertie en énergie. "

    qso_intensity_verylow:0 "QS-O : charge très faible"
    qso_intensity_verylow:0 "Le QS-O se charge très lentement, une grande partie de sa puissance est convertie en énergie. "

    qso_intensity_low:0 "QS-O : faible charge"
    qso_intensity_low_desc:0 "Le QS-O se charge lentement, une partie de sa sortie est convertie en énergie. "

    qso_intensity_high:0 "QS-O : charge élevée"
    qso_intensity_high_desc:0 "Le QS-O est légèrement surchargé, de l'énergie est dépensée pour augmenter sa puissance. "

    qso_intensity_veryhigh:0 "QS-O : charge très élevée"
    qso_intensity_veryhigh_desc:0 "Le QS-O est surchargé, beaucoup d'énergie est utilisée pour augmenter sa puissance. "

    qso_intensity_extremely:0 "QS-O : charge extrême"
    qso_intensity_extremely_desc:0 "Le QS-O est beaucoup surchargé, des quantités massives d'énergie sont dépensées pour augmenter sa production. "

    qso_intensity_insane:0 "QS-O : chargement insensé"
    qso_intensity_insane_desc:0 "Le QS-O est incroyablement surchargé, d'énormes quantités d'énergie sont nécessaires pour le maintenir. "

    qso_intensity_physicslimits:0 "QS-O : Chargement 'Oui'"
    qso_intensity_physicslimits_desc:0 "Le QS-O est aussi surchargé que possible. "

